{
	"name": "background",
	"version": "1.0.1",
	"scripts": {
		"clean": "rimraf -rf ./xgjPlugin",
		"cleanHotfix": "rimraf -rf ./hotFix",
		"dev": "cross-env NODE_ENV=development webpack --watch --config ./build/webpack.config.js  --hide-modules --mode development",
		"dev:build": "cross-env NODE_ENV=devBuild webpack --watch --config ./build/webpack.config.js  --hide-modules --mode development",
		"dev:hotfix": "cross-env NODE_ENV=hotFix webpack --watch --config ./build/webpack.config.js  --hide-modules --mode development",
		"build": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --hide-modules --mode production",
		"test": "npm run clean && cross-env NODE_ENV=test webpack --config ./build/webpack.config.js --hide-modules --mode production",
		"hotfix": "npm run cleanHotfix && cross-env NODE_ENV=production webpack --config ./build/webpack.hotfix.js --hide-modules --mode production",
		"hotfix:debug": "npm run cleanHotfix && cross-env NODE_ENV=production webpack --config ./build/webpack.hotfix.js --hide-modules --mode development",
		"pack": "cross-env NODE_ENV=packPhotoEditor webpack --config ./build/webpack.config.js --hide-modules --mode production",
		"deploy": "node build/deploy.js",
		"deploy:debug": "node --inspect-brk build/deploy.js"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.2.2",
		"@babel/plugin-transform-runtime": "^7.2.0",
		"@babel/preset-env": "^7.2.3",
		"@babel/preset-react": "^7.0.0",
		"@babel/runtime": "^7.2.0",
		"autoprefixer": "^9.4.4",
		"babel-loader": "^8.0.0",
		"babel-plugin-dynamic-import-webpack": "^1.1.0",
		"chalk": "^2.4.1",
		"copy-webpack-plugin": "^4.6.0",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.1",
		"file-loader": "^3.0.1",
		"form-data": "^2.3.3",
		"fs-extra": "^7.0.1",
		"glob": "^7.1.3",
		"html-webpack-plugin": "^3.2.0",
		"invariant": "^2.2.4",
		"json-server": "^0.14.2",
		"less": "^3.9.0",
		"less-loader": "^4.1.0",
		"ora": "^3.0.0",
		"postcss-each": "^0.10.0",
		"postcss-import": "^11.1.0",
		"postcss-loader": "^2.1.6",
		"postcss-url": "^7.3.2",
		"precss": "^3.1.2",
		"rimraf": "^2.6.2",
		"style-loader": "^0.21.0",
		"url-loader": "^1.1.2",
		"vue-loader": "^15.4.2",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.5.21",
		"webpack": "^4.12.0",
		"webpack-chrome-extension-reloader": "^1.1.1",
		"webpack-cli": "^3.0.6",
		"webpack-merge": "^4.2.1"
	},
	"dependencies": {
		"awe-dnd": "^0.3.1",
		"axios": "^0.18.0",
		"canvg": "^1.5.3",
		"clipboard": "^2.0.1",
		"contra": "^1.9.4",
		"core-js": "^2.6.2",
		"crossvent": "^1.5.5",
		"html2canvas": "^1.0.0-alpha.12",
		"iview": "^3.3.3",
		"jspdf": "^1.5.3",
		"lodash": "^4.17.11",
		"moment": "^2.22.2",
		"pako": "^1.0.7",
		"qrcode": "^1.2.2",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-photo-layout-editor": "git+https://gitee.com/HaiMou1990/react-photo-layout-editor-zh-cn.git",
		"smooth-scrollbar": "^8.3.1",
		"vue": "^2.5.21",
		"vue-router": "^3.0.2",
		"vue-wordcloud": "^1.1.1",
		"vue2-slot-calendar": "^2.1.4",
		"xlsx": "^0.14.1"
	}
}
